{
  "name": "verna-examples-poll",
  "private": true,
  "version": "1.0.0-beta.1",
  "scripts": {
    "build": "tsc && vite build",
    "verna:build:check": "yarn workspace @openfun/verna build:check",
    "start": "vite",
    "i18n:extract": "yarn formatjs extract --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format crowdin './src/**/*.ts*' --out-file ./i18n/frontend.json",
    "i18n:compile": "yarn formatjs compile-folder --format crowdin ./i18n/locales ./src/translations",
    "lint:stylelint": "stylelint 'src/**/*.scss' --fix",
    "prelint:eslint": "yarn verna:build:check",
    "lint:eslint": "eslint 'src/**/*.{ts,tsx}'",
    "lint": "yarn lint:stylelint && yarn lint:eslint",
    "test": "jest"
  },
  "dependencies": {
    "@openfun/verna": "1.0.0-beta.1",
    "@rjsf/core": "4.2.0",
    "grommet": "2.24.0",
    "grommet-icons": "4.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "6.2.1",
    "react-is": "16.8.0",
    "styled-components": "5.3.5",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/preset-react": "7.18.6",
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.17",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "@vitejs/plugin-react": "1.3.2",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "jest": "29.3.1",
    "postcss": "8.3.3",
    "sass": "1.53.0",
    "stylelint": "14.9.1",
    "stylelint-config-standard": "26.0.0",
    "stylelint-config-standard-scss": "4.0.0",
    "stylelint-order": "5.0.0",
    "typescript": "4.6.4",
    "uuid": "8.3.2",
    "vite": "3.2.2"
  }
}
